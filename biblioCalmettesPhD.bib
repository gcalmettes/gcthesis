	Mesh = {Animals; Biomechanics; Creatine Kinase; Energy Metabolism; Ferrets; Hypertrophy, Right Ventricular; Male; Mitochondria, Heart; Myocardial Contraction; Myofibrils; Pulmonary Artery},
	Month = {Dec},
	Number = {12},
	Pages = {1573-86},
	Pmid = {7731052},
	Pst = {ppublish},
