	Mesh = {Adenosine Triphosphatases; Animals; Anoxia; Blotting, Southern; Cell Nucleus; Chronic Disease; DNA; DNA, Mitochondrial; Energy Metabolism; Heart Ventricles; Hypertension, Pulmonary; Hypertrophy, Right Ventricular; Male; Microscopy, Electron; Mitochondria, Heart; Oxidative Phosphorylation; Rats; Rats, Wistar; Time Factors},
	Month = {Apr},
