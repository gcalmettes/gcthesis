	Journal = {J Am Coll Cardiol},
	Journal-Full = {Journal of the American College of Cardiology},
	Mesh = {Aged; Coronary Artery Bypass; Coronary Disease; Disease Progression; Female; Fibrosis; Humans; Male; Middle Aged; Myocardial Contraction; Myocardial Reperfusion Injury; Myocardial Stunning; Myocardium; Postoperative Period; Time Factors},
