	Month = {Mar},
	Number = {2},
	Pages = {313-9},
	Pmid = {2156698},
	Pst = {ppublish},
	Title = {Analysis of the control of respiration rate, phosphorylation rate, proton leak rate and protonmotive force in isolated mitochondria using the 'top-down' approach of metabolic control theory},
