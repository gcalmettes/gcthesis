	Journal = {Am J Physiol Regul Integr Comp Physiol},
	Journal-Full = {American journal of physiology. Regulatory, integrative and comparative physiology},
	Mesh = {Algorithms; Animals; Calcium; Calcium Signaling; Cyanides; Cytosol; Glucose; Heart Rate; Magnetic Resonance Spectroscopy; Male; Mitochondria, Heart; Myocardial Contraction; Phosphocreatine; Pyruvic Acid; Rats; Rats, Sprague-Dawley},
	Month = {Jul},
	Number = {1},
	Pages = {R13-9},
	Pmid = {16971375},
	Title = {Modular regulation analysis of heart contraction: application to in situ demonstration of a direct mitochondrial activation by calcium in beating heart},
	Volume = {293},
	Year = {2007},
