	Journal-Full = {The Journal of physiology},
	Mesh = {Adenosine Triphosphate; Animals; Biological Transport; Computer Simulation; Dogs; Energy Metabolism; Hydrolysis; Myocardial Ischemia; Myoglobin; Oxygen; Phosphates},
