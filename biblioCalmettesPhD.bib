	Mesh = {Aminolevulinic Acid; Animals; Cells, Cultured; Flow Cytometry; Heart; Male; Microscopy, Fluorescence; Mitochondria, Heart; Oxygen Consumption; Photosensitizing Agents; Protoporphyrins; Rats; Rats, Wistar},
	Month = {Jun},
