	Journal = {Biochem Biophys Res Commun},
	Journal-Full = {Biochemical and biophysical research communications},
	Mesh = {Adenosine Diphosphate; Adenosine Triphosphate; Animals; Creatine Kinase; Cytosol; Heart; Intracellular Membranes; Isoenzymes; Kinetics; Mitochondria, Heart; Models, Cardiovascular; Models, Theoretical; Myocardium; Myofibrils; Myosins; Oxygen Consumption; Proton-Translocating ATPases},
	Month = {Oct},
